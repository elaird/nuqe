    ,340.283
    ,340.283
    ,340.283
    ,340.283
    ,340.283
    ,340.283};



  Double_t x_sm_Q2_coord_min=142.84;
  Double_t x_sm_Q2_coord_max=447.839;
  Double_t y_sm_Q2_coord_min=340.28;
  Double_t y_sm_Q2_coord_max=516.28;

  Double_t x_sm_Q2_units_min=0.0;
  Double_t x_sm_Q2_units_max=1.4;
  Double_t y_sm_Q2_units_min=0.0;
  Double_t y_sm_Q2_units_max=14.0e-38;

  Double_t x_as_mf_coord_min=147.84;
  Double_t x_as_mf_coord_max=447.839;
  Double_t y_as_mf_coord_min=340.28;
  Double_t y_as_mf_coord_max=516.28;

  Double_t x_as_mf_units_min=0.1;
  Double_t x_as_mf_units_max=0.8;
  Double_t y_as_mf_units_min=0.0;
  Double_t y_as_mf_units_max=2.5e-37;

  Double_t x_as_corr_coord_min=x_as_mf_coord_min;
  Double_t x_as_corr_coord_max=x_as_mf_coord_max;
  Double_t y_as_corr_coord_min=y_as_mf_coord_min;
  Double_t y_as_corr_coord_max=y_as_mf_coord_max;

  Double_t x_as_corr_units_min=x_as_mf_units_min;
  Double_t x_as_corr_units_max=x_as_mf_units_max;
  Double_t y_as_corr_units_min=y_as_mf_units_min;
  Double_t y_as_corr_units_max=y_as_mf_units_max;

  Double_t x_n_corr_coord_min=162.84;
  Double_t x_n_corr_coord_max=447.839;
  Double_t y_n_corr_coord_min=340.28;
  Double_t y_n_corr_coord_max=516.28;

  Double_t x_n_corr_units_min=0.0;
  Double_t x_n_corr_units_max=0.8;
  Double_t y_n_corr_units_min=-5.0;
  Double_t y_n_corr_units_max=log(5.0e-1)/log(10.0);

  Double_t x_n_mf_coord_min=x_n_corr_coord_min;
  Double_t x_n_mf_coord_max=x_n_corr_coord_max;
  Double_t y_n_mf_coord_min=y_n_corr_coord_min;
  Double_t y_n_mf_coord_max=y_n_corr_coord_max;

  Double_t x_n_mf_units_min=x_n_corr_units_min;
  Double_t x_n_mf_units_max=x_n_corr_units_max;
  Double_t y_n_mf_units_min=y_n_corr_units_min;
  Double_t y_n_mf_units_max=y_n_corr_units_max;

  Double_t x_sm_pb_coord_min=147.84;
  Double_t x_sm_pb_coord_max=447.839;
  Double_t y_sm_pb_coord_min=340.28;
  Double_t y_sm_pb_coord_max=516.28;

  Double_t x_sm_pb_units_min=0.1;
  Double_t x_sm_pb_units_max=0.8;
  Double_t y_sm_pb_units_min=0.0;
  Double_t y_sm_pb_units_max=3.0e-40;

  Double_t x_sm_no_pb_coord_min=x_sm_pb_coord_min;
  Double_t x_sm_no_pb_coord_max=x_sm_pb_coord_max;
  Double_t y_sm_no_pb_coord_min=y_sm_pb_coord_min;
  Double_t y_sm_no_pb_coord_max=y_sm_pb_coord_max;

  Double_t x_sm_no_pb_units_min=x_sm_pb_units_min;
  Double_t x_sm_no_pb_units_max=x_sm_pb_units_max;
  Double_t y_sm_no_pb_units_min=y_sm_pb_units_min;
  Double_t y_sm_no_pb_units_max=y_sm_pb_units_max;

  Double_t x_nopb_e_as_coord_min=147.84;
  Double_t x_nopb_e_as_coord_max=447.839;
  Double_t y_nopb_e_as_coord_min=340.28;
  Double_t y_nopb_e_as_coord_max=516.28;

  Double_t x_nopb_e_as_units_min=0.1;
  Double_t x_nopb_e_as_units_max=0.8;
  Double_t y_nopb_e_as_units_min=0.0;
  Double_t y_nopb_e_as_units_max=3.0e-40;

  Double_t x_pb_e_as_coord_min=147.84;
  Double_t x_pb_e_as_coord_max=447.839;
  Double_t y_pb_e_as_coord_min=340.28;
  Double_t y_pb_e_as_coord_max=516.28;

  Double_t x_pb_e_as_units_min=0.1;
  Double_t x_pb_e_as_units_max=0.8;
  Double_t y_pb_e_as_units_min=0.0;
  Double_t y_pb_e_as_units_max=3.0e-40;

  Double_t x_benhar_q2_coord_min=142.84;
  Double_t x_benhar_q2_coord_max=447.839;
  Double_t y_benhar_q2_coord_min=340.28;
  Double_t y_benhar_q2_coord_max=516.28;

  Double_t x_benhar_q2_units_min=0.0;
  Double_t x_benhar_q2_units_max=1.4;
  Double_t y_benhar_q2_units_min=0.0;
  Double_t y_benhar_q2_units_max=14.0e-38;

  Double_t x_FG_q2_coord_min=x_benhar_q2_coord_min;
  Double_t x_FG_q2_coord_max=x_benhar_q2_coord_max;
  Double_t y_FG_q2_coord_min=y_benhar_q2_coord_min;
  Double_t y_FG_q2_coord_max=y_benhar_q2_coord_max;

  Double_t x_FG_q2_units_min=x_benhar_q2_units_min;
  Double_t x_FG_q2_units_max=x_benhar_q2_units_max;
  Double_t y_FG_q2_units_min=y_benhar_q2_units_min;
  Double_t y_FG_q2_units_max=y_benhar_q2_units_max;

  for (Int_t i=0;i<N_sm_pb;i++) {
    x_sm_pb[i]=(x_sm_pb[i]-x_sm_pb_coord_min)/(x_sm_pb_coord_max-x_sm_pb_coord_min)*(x_sm_pb_units_max-x_sm_pb_units_min)+x_sm_pb_units_min;
    y_sm_pb[i]=(y_sm_pb[i]-y_sm_pb_coord_min)/(y_sm_pb_coord_max-y_sm_pb_coord_min)*(y_sm_pb_units_max-y_sm_pb_units_min)+y_sm_pb_units_min;

    //convert to my units
    y_sm_pb[i]*=1.0e3;
  }

  for (Int_t i=0;i<N_sm_no_pb;i++) {
    x_sm_no_pb[i]=(x_sm_no_pb[i]-x_sm_no_pb_coord_min)/(x_sm_no_pb_coord_max-x_sm_no_pb_coord_min)*(x_sm_no_pb_units_max-x_sm_no_pb_units_min)+x_sm_no_pb_units_min;
    y_sm_no_pb[i]=(y_sm_no_pb[i]-y_sm_no_pb_coord_min)/(y_sm_no_pb_coord_max-y_sm_no_pb_coord_min)*(y_sm_no_pb_units_max-y_sm_no_pb_units_min)+y_sm_no_pb_units_min;

    //convert to my units
    y_sm_no_pb[i]*=1.0e3;
  }

  for (Int_t i=0;i<N_sm_Q2;i++) {
    x_sm_Q2[i]=(x_sm_Q2[i]-x_sm_Q2_coord_min)/(x_sm_Q2_coord_max-x_sm_Q2_coord_min)*(x_sm_Q2_units_max-x_sm_Q2_units_min)+x_sm_Q2_units_min;
    y_sm_Q2[i]=(y_sm_Q2[i]-y_sm_Q2_coord_min)/(y_sm_Q2_coord_max-y_sm_Q2_coord_min)*(y_sm_Q2_units_max-y_sm_Q2_units_min)+y_sm_Q2_units_min;

    //already in my units
  }

  for (Int_t i=0;i<N_as_mf;i++) {
    x_as_mf[i]=(x_as_mf[i]-x_as_mf_coord_min)/(x_as_mf_coord_max-x_as_mf_coord_min)*(x_as_mf_units_max-x_as_mf_units_min)+x_as_mf_units_min;
    y_as_mf[i]=(y_as_mf[i]-y_as_mf_coord_min)/(y_as_mf_coord_max-y_as_mf_coord_min)*(y_as_mf_units_max-y_as_mf_units_min)+y_as_mf_units_min;

    //already in my units
  }

  for (Int_t i=0;i<N_as_corr;i++) {
    x_as_corr[i]=(x_as_corr[i]-x_as_corr_coord_min)/(x_as_corr_coord_max-x_as_corr_coord_min)*(x_as_corr_units_max-x_as_corr_units_min)+x_as_corr_units_min;
    y_as_corr[i]=(y_as_corr[i]-y_as_corr_coord_min)/(y_as_corr_coord_max-y_as_corr_coord_min)*(y_as_corr_units_max-y_as_corr_units_min)+y_as_corr_units_min;

    //already in my units
  }

  for (Int_t i=0;i<N_n_corr;i++) {
    x_n_corr[i]=(x_n_corr[i]-x_n_corr_coord_min)/(x_n_corr_coord_max-x_n_corr_coord_min)*(x_n_corr_units_max-x_n_corr_units_min)+x_n_corr_units_min;
    y_n_corr[i]=(y_n_corr[i]-y_n_corr_coord_min)/(y_n_corr_coord_max-y_n_corr_coord_min)*(y_n_corr_units_max-y_n_corr_units_min)+y_n_corr_units_min;

    Double_t f_fm_conv=0.197327;//GeV.fm
    y_n_corr[i]=pow(10.0,y_n_corr[i])*pow(f_fm_conv,-3.0)*8;
  }

  for (Int_t i=0;i<N_n_mf;i++) {
    x_n_mf[i]=(x_n_mf[i]-x_n_mf_coord_min)/(x_n_mf_coord_max-x_n_mf_coord_min)*(x_n_mf_units_max-x_n_mf_units_min)+x_n_mf_units_min;
    y_n_mf[i]=(y_n_mf[i]-y_n_mf_coord_min)/(y_n_mf_coord_max-y_n_mf_coord_min)*(y_n_mf_units_max-y_n_mf_units_min)+y_n_mf_units_min;

    Double_t f_fm_conv=0.197327;//GeV.fm
    y_n_mf[i]=pow(10.0,y_n_mf[i])*pow(f_fm_conv,-3.0)*8;
  }

  for (Int_t i=0;i<N_nopb_e_as;i++) {
    x_nopb_e_as[i]=(x_nopb_e_as[i]-x_nopb_e_as_coord_min)/(x_nopb_e_as_coord_max-x_nopb_e_as_coord_min)*(x_nopb_e_as_units_max-x_nopb_e_as_units_min)+x_nopb_e_as_units_min;
    y_nopb_e_as[i]=(y_nopb_e_as[i]-y_nopb_e_as_coord_min)/(y_nopb_e_as_coord_max-y_nopb_e_as_coord_min)*(y_nopb_e_as_units_max-y_nopb_e_as_units_min)+y_nopb_e_as_units_min;

    //convert to my units
    y_nopb_e_as[i]*=1.0e3;
  }

  for (Int_t i=0;i<N_pb_e_as;i++) {
    x_pb_e_as[i]=(x_pb_e_as[i]-x_pb_e_as_coord_min)/(x_pb_e_as_coord_max-x_pb_e_as_coord_min)*(x_pb_e_as_units_max-x_pb_e_as_units_min)+x_pb_e_as_units_min;
    y_pb_e_as[i]=(y_pb_e_as[i]-y_pb_e_as_coord_min)/(y_pb_e_as_coord_max-y_pb_e_as_coord_min)*(y_pb_e_as_units_max-y_pb_e_as_units_min)+y_pb_e_as_units_min;

    //convert to my units
    y_pb_e_as[i]*=1.0e3;
  }

  for (Int_t i=0;i<N_benhar_q2;i++) {
    x_benhar_q2[i]=(x_benhar_q2[i]-x_benhar_q2_coord_min)/(x_benhar_q2_coord_max-x_benhar_q2_coord_min)*(x_benhar_q2_units_max-x_benhar_q2_units_min)+x_benhar_q2_units_min;
    y_benhar_q2[i]=(y_benhar_q2[i]-y_benhar_q2_coord_min)/(y_benhar_q2_coord_max-y_benhar_q2_coord_min)*(y_benhar_q2_units_max-y_benhar_q2_units_min)+y_benhar_q2_units_min;
  }

  for (Int_t i=0;i<N_FG_q2;i++) {
    x_FG_q2[i]=(x_FG_q2[i]-x_FG_q2_coord_min)/(x_FG_q2_coord_max-x_FG_q2_coord_min)*(x_FG_q2_units_max-x_FG_q2_units_min)+x_FG_q2_units_min;
    y_FG_q2[i]=(y_FG_q2[i]-y_FG_q2_coord_min)/(y_FG_q2_coord_max-y_FG_q2_coord_min)*(y_FG_q2_units_max-y_FG_q2_units_min)+y_FG_q2_units_min;
  }

  TGraph graph_sm_pb(N_sm_pb,x_sm_pb,y_sm_pb);
  graph_sm_pb.SetName("polish_e_sm");

  TGraph graph_sm_no_pb(N_sm_no_pb,x_sm_no_pb,y_sm_no_pb);
  graph_sm_no_pb.SetName("polish_nopb_e_sm");

  TGraph graph_sm_Q2(N_sm_Q2,x_sm_Q2,y_sm_Q2);
  graph_sm_Q2.SetName("polish_Q2_sm");

  TGraph graph_as_mf(N_as_mf,x_as_mf,y_as_mf);
  graph_as_mf.SetName("polish_e_as_mf");

  TGraph graph_as_corr(N_as_corr,x_as_corr,y_as_corr);
  graph_as_corr.SetName("polish_e_as_corr");

  TGraph graph_n_corr(N_n_corr,x_n_corr,y_n_corr);
  graph_n_corr.SetName("polish_n_corr");
  
  TGraph graph_n_mf(N_n_mf,x_n_mf,y_n_mf);
  graph_n_mf.SetName("polish_n_mf");
  
  TGraph graph_nopb_e_as(N_nopb_e_as,x_nopb_e_as,y_nopb_e_as);
  graph_nopb_e_as.SetName("polish_nopb_e_as");
  
  TGraph graph_pb_e_as(N_pb_e_as,x_pb_e_as,y_pb_e_as);
  graph_pb_e_as.SetName("polish_e_as");
  
  TGraph graph_benhar_q2(N_benhar_q2,x_benhar_q2,y_benhar_q2);
  graph_benhar_q2.SetName("benhar_q2");

  TGraph graph_FG_q2(N_FG_q2,x_FG_q2,y_FG_q2);
  graph_FG_q2.SetName("FG_q2");
  
  TFile f("graph.root","RECREATE");
  graph_sm_pb.Write();
  graph_sm_no_pb.Write();
  graph_sm_Q2.Write();
  graph_as_mf.Write();
  graph_as_corr.Write();
  graph_n_corr.Write();
  graph_n_mf.Write();
  graph_nopb_e_as.Write();
  graph_pb_e_as.Write();
  graph_benhar_q2.Write();
  graph_FG_q2.Write();
  f.Close();
  
}
